@model (List<RottenPotatoes.Models.Review>, RottenPotatoes.Models.User, RottenPotatoes.Models.User );
@{
    ViewData["Title"] = "User's Reviews";
    User usersReview = Model.Item3;
}


<div class="container mt-5">
    <div class="card shadow p-4 rounded-3 mx-auto" style="max-width: 600px;">
        <h2 class="mb-4 text-center">@usersReview?.Login_Hash Reviews</h2>
        @Html.Partial("All", (Model.Item1, Model.Item2))
    </div>
</div>
