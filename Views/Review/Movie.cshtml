@model (List<RottenPotatoes.Models.Review>, RottenPotatoes.Models.User, Movie);
@{
    ViewData["Title"] = "Movie Reviews";
    Movie movie = Model.Item3;
}


<div class="container mt-5">
    <div class="card shadow p-4 rounded-3 mx-auto">
        <h2 class="mb-4 text-center">@movie?.Title Reviews</h2>
        @Html.Partial("_ReviewList", (Model.Item1, Model.Item2))
    </div>
</div>
