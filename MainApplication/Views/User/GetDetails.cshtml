@model User

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>User</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Username
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Login_Hash)
        </dd>
        <dt class = "col-sm-2">
            E-mail
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Email_Hash)
        </dd>
        <dt class = "col-sm-2">
            Api Key
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ApiToken)
        </dd>
        